/*------------hotsport css-----------------*/
.hotspots{
  width: 25px;
  height: 25px;
  background-color:#3AB3EF ;
  border-radius: 50%;
  position: absolute;
  animation: wave 1s infinite;
  cursor: pointer;
}


.experience{
  right: 41%;
  bottom: 35%;
}
.share{
  left: 40.50%;
  bottom: 34%;
}
.hotspots::before{
  content: attr(text);
  width: max-content;
  position: absolute;
  background-color: #222222dd;
  color: #fff;
  left: 50%;
  transform: translateX(-50%);
  top: 30px;
  padding: 8px 20px;
  font-size: 14px;
  border-radius: 3px;
  display: none;
}

.hotspots::after{
  content: "";
  border-color: transparent transparent #222222dd;
  border-width: 0 8px 6px;
  border-style: solid;
  position: absolute;
  top: 24px;
  left: 50%;
  transform: translateX(-50%);
  display: none;
}

.hotspots:hover::before,
.hotspots:hover::after{
  display: block;
}
@media (max-width: 768px) {
  
 
  .hotspots{
    width: 12px;
    height: 12px;
  }
  .experience{
 
  bottom: 36%;
}
.share{
  
  bottom: 36%;
}

}
